		    <h3 id="viewOrdersListTitle">View All Orders</h3>
		</header>   

        <div id="prodcutionsListContent">
            {{#each inventory}}
                <ul>
                    <a href="/inventory/{{_id}}"><li>{{productName}}</li></a>
